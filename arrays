/* 
Will ask user to enter 5 numbers and then prints it out into array 
*/

#include <iostream>
using namespace std;

int main(){

  int const SIZE= 5; // needs data type 
  int arr[i]; //declare array 

  //input for array
  for (int i = 0; i<SIZE; i++)
  {
    cout<<"Enter number: ";
    cin>>arr[i];
  }

  //display for array 
  for (int i = 0;i < SIZE;i++)
  {
    cout<<arr[i]<<" ";
  }

	cout<<endl;

  return 0;
}
